<h3 class="mt-3">Chilling Loccini Blog</h3>
<p>Read our amazing blog posts below...</p>
<% if (blogs.length>0) {%>
    <%blogs.forEach(blog=>{%>
       <a href="/blogs/<%=blog._id%>" style="text-decoration: none;"> <h4><%=blog.title%></h4></a>
        <p><%=blog.snippet ?? 'd' %></p>
    <%})%>
<% } else {%>
    <h3>No blog</h3>
<% } %>